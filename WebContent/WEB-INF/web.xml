<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LibraryManagementSystem</display-name>
  <servlet>
    <display-name>Borrower</display-name>
    <servlet-name>Borrower</servlet-name>
    <servlet-class>app.controller.BorrowerControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>Fines</display-name>
    <servlet-name>Fines</servlet-name>
    <servlet-class>app.controller.FineControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>BookSearch</display-name>
    <servlet-name>BookSearch</servlet-name>
    <servlet-class>app.controller.BookSearchServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>CheckIn</display-name>
    <servlet-name>CheckIn</servlet-name>
    <servlet-class>app.controller.CheckInControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>CheckOut</display-name>
    <servlet-name>CheckOut</servlet-name>
    <servlet-class>app.controller.CheckOutControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>Success</display-name>
    <servlet-name>Success</servlet-name>
    <servlet-class>lms.Success</servlet-class>
  </servlet>
  <servlet>
    <display-name>Error</display-name>
    <servlet-name>Error</servlet-name>
    <servlet-class>lms.Error</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Borrower</servlet-name>
    <url-pattern>/CreateBorrower</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Fines</servlet-name>
    <url-pattern>/Fines</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BookSearch</servlet-name>
    <url-pattern>/BookSearch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckIn</servlet-name>
    <url-pattern>/CheckIn</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckOut</servlet-name>
    <url-pattern>/CheckOut</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Success</servlet-name>
    <url-pattern>/Success</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Error</servlet-name>
    <url-pattern>/Error</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
</web-app>