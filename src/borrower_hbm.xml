<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="app.model.Borrower" table="BORROWER">
        <id column="CARD_ID" name="cardId" type="java.lang.Integer" />
        <property column="BNAME" name="bname" type="java.lang.String" />
        <property column="SSN" name="ssn" type="java.lang.String" />
        <property column="ADDRESS" name="address" type="java.lang.String" />
        <property column="PHONE" name="phone" type="java.lang.String" />
    </class>
</hibernate-mapping>